# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file and UTF-8 encoding
[*]
max_line_length = 140
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 4

[*.java]
indent_size = 4

[{*.yml,*.yaml}]
indent_size = 2

[{*.js,*.jsx}]
indent_size = 2

[{*.ts,*.tsx}]
indent_size = 2

[*.json]
indent_size = 2

[index.html]
indent_size = 4
